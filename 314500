public class Date{

  private int day;
  private int month;
  private int year;

	Date(int day,int month,int year)
	{
	   this.day = day;
	   this.month = month;
	   this.year = year;
	} 

	public String toString()
	{
	    return String.format("%02d-%02d-%02d",day,month,year);
	}
}